#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/my-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/my-blog/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../tty-table@4.1.6/node_modules/tty-table/adapters/terminal-adapter.js" "$@"
else
  exec node  "$basedir/../../../../../../tty-table@4.1.6/node_modules/tty-table/adapters/terminal-adapter.js" "$@"
fi
